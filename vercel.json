{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
    /*"env"{
        "API_KEY": "@api_key",
        "AUDIENCE_ID": "@audience_id",
        "MAILCHIMP_SERVER_PREFIX": "@mailchimp_server_prefix",
        "REMOTE_HOST": "@remote_host",
        "SMTP_HOST": "@smtp_host",
        "SMTP_PORT": "@smtp_port",
        "SMTP_USER": "@smtp_user",
        "SMTP_PASS": "@smtp_pass",
        "REMOTE_HOST": "@remote_host"
      }*/
  }